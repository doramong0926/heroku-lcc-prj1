<%- layout('layout.admin') -%>

<div class="main main-raised">
    <div class="container fist-container" id="container-admin-icoSchedule">
        <div class="section text-center">
            <div class="features">
                <div class="row">
                    <div class="col-12 text-left">
                        <h2 class="title"># ICO INFO</h2> 
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left" >Token Name : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-tokenName"></h4>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Contract Address : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-contractAddress"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Ico Address : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-icoAddress"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>

                <div class="row">
                    <div class="col-12 text-left">
                        <h2 class="title"># ICO SALES VOLUME</h2> 
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left" >Total sales volume(BLC) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-totalSalesVolume"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-totalSalesVolume">Unlock</button>
                        <button class="btn btn-primary btn-sm" id="save-totalSalesVolume" style="display:none;">Save</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">PreSale volume(BLC) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-preSalesVolume"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Round-A volume(BLC) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-roundAVolume"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Round-B volume(BLC) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-roundBVolume"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Round-C volume(BLC) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-roundCVolume"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>


                <div class="row">
                    <div class="col-12 text-left">
                        <h2 class="title"># ICO SCHEDULE</h2> 
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Ico current round : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-round"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Start of PreSale Date : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-startPreSale"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">End of PreSale Date : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-endPreSale"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">End of RoundA Date : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-endRoundA"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">End of RoundB Date : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-endRoundB"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">End of RoundC Date : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-endRoundC"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>

                <div class="row">
                    <div class="col-12 text-left">
                        <h2 class="title"># ICO BONUS RATE</h2> 
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Ico exchage rate(BLC) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-exchangeRate"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Bonus of PreSale(%) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-bonusPreSale"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Bonus of RoundA(%) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-bonusRoundA"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Bonus of RoundB(%) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-bonusRoundB"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Bonus of RoundC(%) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-bonusRoundC"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Bonus of Volume - 10Eth(%) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-bonusVolume10Eth"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Bonus of Volume - 30Eth(%) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-bonusVolume30Eth"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Bonus of Volume - 50Eth(%) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-bonusVolume50Eth"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
                <div class="row">
                    <h4 class="col-sm-3 col-form-label text-left">Bonus of Referral(%) : </h4>   
                    <div class="col-sm-7">
                        <h4><input type="text" readonly class="form-control-plaintext" id="admin-bonusReferral"></h4>
                    </div>
                    <div class="col-sm-2  text-left">
                        <button class="btn btn-primary btn-sm" id="unlock-icoInfo">Unlock</button>
                    </div>
                </div>
            </div>                
        </div>
    </div>
</div>
